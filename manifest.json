{
  "name": "test",
  "manifest_version": 2,
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "p5.js",
        "p5.sound.js",
        "lottie.js",
        "sketch.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "flowericonv2.png",
    "default_title": "Internet Sanctuary",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab"
  ]
}